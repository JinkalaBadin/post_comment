<h1> Post Index List</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= link_to post.title, post%></td><br>
        <td><%= post.content %></td>
      </tr>
    <% end %>
  </tbody>
</table> 


